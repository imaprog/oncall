<h1>Engineers#index</h1>
<p>Find me in app/views/engineers/index.html.erb</p>
<table border="1">
<thead align="center">
	<tr>
		<th>Name</th>
		<th>On-call hours this month</th>
	</tr>
</thead>	
<tbody align="center">
	<% @engineers.each do |engineer| %>
		<tr>
			<td><%= engineer.name %></td>
			<td><%= engineer.overtimes.count(:hours) %></td>
		</tr>
	<% end %>
</tbody>
</table>
